#include<stdio.h>
#include<time.h>
int main{
int coin[3], heads=0, tails=0, h=0, t=0;

print("Tosshing a coin...\n");
srand(time(NULL));

for(int i=0;i<3;i++){
coin[i]=rand() % 2;
print("Round%d:",i);

if(coin[i]==0) {
printf("Heads\n");
h++;
}

if(coin[i]==1) {
printf("Tails\n");
t++;
}
}
pritf("Head: %d",h);
pritf(",Tails: %d\n",t);
return 0;
}
